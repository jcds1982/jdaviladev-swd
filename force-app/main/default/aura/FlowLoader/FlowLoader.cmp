<aura:component implements="flexipage:availableForRecordHome,force:hasRecordId" access="global">
    <aura:handler name="init" value="{!this}" action="{!c.init}" />
    <aura:attribute name="recordId" type="String" />
    
    <aura:attribute name="opportunityRecord" type="Object"/>
    <aura:attribute name="accountRecord" type="Object"/>
    <aura:attribute name="recordLoadError" type="String"/>
    <aura:attribute name="FValues" type="String[]"/>
    
    <force:recordData aura:id="recordLoader"
    recordId="{!v.recordId}"
    fields="Id, Name, AccountId, Account.Name,Account.Active__c"
    targetFields="{!v.opportunityRecord}"
    targetError="{!v.recordLoadError}"
    recordUpdated="{!c.recordUpdate}" />
  
    
    FValues: {!v.FValues[0]}    ### 
    
    
    1234 - {!v.opportunityRecord.Name} - {!v.opportunityRecord.Account.Name}
    
    <p class="slds-text-heading--small">
                <lightning:formattedText title="Account Name" value="{!v.opportunityRecord.Name}" />
    </p>
    
    
    
    
    
    
    <!-- <lightning:overlayLibrary aura:id="overlayLib" /> -->
	{!v.recordId}    
    <div>
        <lightning:flow aura:id="flowData" />
    </div>
	
</aura:component>